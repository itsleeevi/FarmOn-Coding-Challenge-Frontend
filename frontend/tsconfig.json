{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true, 
    "plugins": [
      {
        "name": "next"
      }
    ],"types": ["cypress"],
    "paths": {
      "@/*": ["./*"]
    }, 
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts",
  "src/**/*.ts",
 "cypress/**/*.ts","cypress/**/*.ts", "cypress/tsconfig.json", "cypress.config.js"],
  "exclude": ["node_modules"]
}
